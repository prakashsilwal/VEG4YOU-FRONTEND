<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation2"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.merokisanbazar.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.merokisanbazar.ui.dashboard.DashboardFragment"
        android:label="Products"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.merokisanbazar.ui.notifications.NotificationsFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/nav_user"
        android:name="com.example.merokisanbazar.fragments.UserFragment"
